{
    "name": "financial-advisor-ai-agent",
    "version": "1.0.0",
    "description": "AI agent for Financial Advisors with Gmail, Calendar, and HubSpot integration",
    "main": "dist/server/index.js",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "nodemon --watch src --exec \"ts-node --esm src/server/index.ts\"",
        "dev:client": "cd client && npm run dev",
        "build": "npm run build:server && npm run build:client",
        "build:server": "tsc -p tsconfig.server.json",
        "build:client": "cd client && npm run build",
        "start": "node dist/server/index.js",
        "install:all": "npm install && cd client && npm install",
        "db:migrate": "npx prisma migrate dev",
        "db:generate": "npx prisma generate",
        "db:studio": "npx prisma studio",
        "db:push": "npx prisma db push --accept-data-loss"
    },
    "keywords": [
        "ai",
        "financial-advisor",
        "gmail",
        "calendar",
        "hubspot"
    ],
    "author": "Financial Advisor AI Agent",
    "license": "MIT",
    "dependencies": {
        "@hubspot/api-client": "^9.0.0",
        "@huggingface/inference": "^4.5.1",
        "@langchain/community": "^0.3.48",
        "@langchain/core": "^0.3.62",
        "@langchain/groq": "^0.2.3",
        "@prisma/client": "^5.6.0",
        "axios": "^1.6.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "express-session": "^1.17.3",
        "google-auth-library": "^9.0.0",
        "googleapis": "^128.0.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "langchain": "^0.3.29",
        "node-cache": "^5.1.2",
        "node-cron": "^3.0.3",
        "openai": "^4.20.0",
        "passport": "^0.7.0",
        "passport-google-oauth20": "^2.0.0",
        "pg": "^8.11.3",
        "pgvector": "^0.2.0",
        "prisma": "^5.6.0",
        "wait-port": "^1.1.0",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-session": "^1.17.10",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.8.0",
        "@types/passport": "^1.0.16",
        "@types/passport-google-oauth20": "^2.0.0",
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}